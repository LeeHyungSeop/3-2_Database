	vID int, 
	vName text NOT NULL,
	DOB datetime DEFAULT CURRENT_TIMESTAMP NOT NULL, 
	sName TEXT DEFAULT &quot;고졸&quot;, 
	phoneNum TEXT NOT NULL, 
	accumCNT INT DEFAULT 0,
	
	PRIMARY KEY (vID)
