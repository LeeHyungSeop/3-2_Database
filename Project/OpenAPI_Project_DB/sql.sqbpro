CREATE TABLE WELFARECENTER(
	wcName int, 
	phoneNum TEXT NOT NULL, 
	address TEXT NOT NULL,
	rating INT DEFAULT 0,
	
	PRIMARY KEY (wcName)
)

CREATE TABLE SERVICES(
	sNum int , 
	vName TEXT NOT NULL, 
	vPhoneNum TEXT NOT NULL,
	sLoc TEXT,
	isFinish TEXT DEFAULT 'N',
	isAssign TEXT DEFAULT 'N',
	wcName TEXT,
	vID int,
	
	PRIMARY KEY (sNum)
	FOREIGN KEY (wcName) REFERENCES WELFARECENTER(wcName) on DELETE CASCADE on UPDATE CASCADE,
	FOREIGN KEY (vID) REFERENCES VOLUNTEERS(vID) on DELETE CASCADE on UPDATE CASCADE
